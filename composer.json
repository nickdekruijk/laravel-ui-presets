{
    "name": "nickdekruijk/laravel-ui-presets",
    "description": "Frontend presets for Laravel",
    "keywords": ["minify", "css", "sass", "view", "laravel", "preset", "ui", "frontend"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nickdekruijk/laravel-ui-presets/issues",
        "source": "https://github.com/nickdekruijk/laravel-ui-presets/sections-preset/"
    },
    "authors": [{
        "name": "Nick de Kruijk",
        "email": "git@nickdekruijk.nl"
    }],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "laravel/ui": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "NickDeKruijk\\LaravelUIPresets\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NickDeKruijk\\LaravelUIPresets\\PresetsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
